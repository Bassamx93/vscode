{
  "name": "hello-world",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.0.0",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "hello",
    "world",
    "example",
    "sample"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "activationEvents": [
    "onCommand:helloWorld.hello"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "helloWorld.hello",
        "title": "Hello World",
        "category": "Hello World"
      }
    ]
  },
  "scripts": {
    "compile": "gulp compile-extension:hello-world",
    "watch": "gulp watch-extension:hello-world"
  },
  "devDependencies": {
    "@types/node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode.git"
  }
}