{
	"name": "hello-world",
	"displayName": "Hello World",
	"description": "A simple hello world extension",
	"version": "1.0.0",
	"publisher": "vscode",
	"license": "MIT",
	"engines": {
		"vscode": "^1.5.0"
	},
	"icon": "media/icon.png",
	"activationEvents": [
		"onCommand:hello-world.helloWorld",
		"onCommand:hello-world.showWorkspaceInfo"
	],
	"main": "./out/extension",
	"scripts": {
		"compile": "gulp compile-extension:hello-world",
		"watch": "gulp watch-extension:hello-world",
		"audit-fix": "npm audit fix"
	},
	"contributes": {
		"commands": [
			{
				"command": "hello-world.helloWorld",
				"title": "Hello World",
				"category": "Hello"
			},
			{
				"command": "hello-world.showWorkspaceInfo",
				"title": "Show Workspace Info",
				"category": "Hello"
			}
		],
		"keybindings": [
			{
				"command": "hello-world.helloWorld",
				"key": "ctrl+shift+h",
				"mac": "cmd+shift+h",
				"when": "editorTextFocus"
			}
		]
	},
	"devDependencies": {
		"@types/node": "22.x"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode.git"
	}
}